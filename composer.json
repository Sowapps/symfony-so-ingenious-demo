{
    "type": "project",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "../symfony-so-core-bundle"
        },
        {
            "type": "path",
            "url": "../symfony-so-log-bundle"
        },
        {
            "type": "path",
            "url": "../symfony-so-ingenious-bundle"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "beberlei/doctrineextensions": ">=1.5",
        "doctrine/dbal": "^3.10.4",
        "doctrine/doctrine-bundle": "^2.18.2",
        "doctrine/doctrine-migrations-bundle": "^3.7.0",
        "doctrine/orm": "^3.6.1",
        "jolicode/automapper": ">=9.5",
        "league/commonmark": "^2.8.0",
        "phpdocumentor/reflection-docblock": "^5.6.6",
        "phpstan/phpdoc-parser": "^2.3",
        "sowapps/so-core": "*",
        "sowapps/so-ingenious": "*",
        "symfony/apache-pack": ">=1.0.1",
        "symfony/asset": "^7.4",
        "symfony/asset-mapper": "^7.4.3",
        "symfony/console": "^7.4.3",
        "symfony/doctrine-messenger": "^7.4.3",
        "symfony/dotenv": "^7.4",
        "symfony/expression-language": "^7.4",
        "symfony/flex": "^2.10.0",
        "symfony/form": "^7.4.3",
        "symfony/framework-bundle": "^7.4.3",
        "symfony/http-client": "^7.4.3",
        "symfony/intl": "^7.4",
        "symfony/mailer": "^7.4.3",
        "symfony/mime": "^7.4",
        "symfony/monolog-bundle": "^3.11.1",
        "symfony/notifier": "^7.4",
        "symfony/process": "^7.4.3",
        "symfony/property-access": "^7.4.3",
        "symfony/property-info": "^7.4.3",
        "symfony/runtime": "^7.4.1",
        "symfony/security-bundle": "^7.4",
        "symfony/serializer": "^7.4.3",
        "symfony/stimulus-bundle": "^2.32",
        "symfony/string": "^7.4",
        "symfony/translation": "^7.4.3",
        "symfony/twig-bundle": "^7.4.3",
        "symfony/ux-turbo": "^2.32",
        "symfony/validator": "^7.4.3",
        "symfony/web-link": "^7.4",
        "symfony/yaml": "^7.4.1",
        "symfonycasts/sass-bundle": "^0.8.3",
        "twig/extra-bundle": "^2.12|^3.22.2",
        "twig/twig": "^2.12|^3.22.2"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^8.4.1",
        "doctrine/doctrine-fixtures-bundle": "^4.3.1",
        "phpunit/phpunit": "^11.5.46",
        "rector/rector": "^2.3.0",
        "symfony/browser-kit": "^7.4.3",
        "symfony/css-selector": "^7.4",
        "symfony/debug-bundle": "^7.4",
        "symfony/maker-bundle": "^1.65.1",
        "symfony/phpunit-bridge": "^7.4.3",
        "symfony/stopwatch": "^7.4",
        "symfony/web-profiler-bundle": "^7.4.3"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Sowapps\\": "src/Sowapps/",
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "^7.4"
        }
    },
    "authors": [
        {
            "name": "Florent HAZARD",
            "email": "contact@sowapps.com",
            "homepage": "https://sowapps.com",
            "role": "Developer"
        }
    ]
}
